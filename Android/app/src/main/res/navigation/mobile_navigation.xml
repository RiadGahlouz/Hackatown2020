<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_resto">

    <fragment
        android:id="@+id/navigation_resto"
        android:name="ca.teamrocket.polyeats.restoFragment.RestoFragment"
        android:label="@string/title_resto"
        tools:layout="@layout/fragment_resto" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="ca.teamrocket.polyeats.searchFragment.SearchFragment"
        android:label="@string/title_search"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/navigation_history"
        android:name="ca.teamrocket.polyeats.historyFragment.HistoryFragment"
        android:label="@string/title_history"
        tools:layout="@layout/fragment_history" />
</navigation>